{
  "name": "doctor",
  "description": "Environment/readiness diagnostics.",
  "defaults": {
    "format": "json"
  },
  "command": "doctor",
  "args": ["--json"],
  "outputSchema": {
    "success": "boolean - overall health status",
    "version": "string - CLI version",
    "environment": "object - node version, platform, cwd",
    "checks": "object - embeddings status, workspace detection",
    "warnings": "array - non-fatal issues detected"
  },
  "notes": [
    "Run this first to verify environment setup",
    "Check 'checks.embeddings.initialized' before using context/debug",
    "Use to troubleshoot CLI issues"
  ]
}
